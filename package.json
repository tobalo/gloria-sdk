{
  "name": "@tobalo/gloria-sdk",
  "version": "0.0.3",
  "description": "TypeScript SDK for Gloria AI News Hub - Real-time financial and crypto news",
  "repository": {
    "type": "git",
    "url": "https://github.com/tobalo/gloria-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/tobalo/gloria-sdk/issues"
  },
  "homepage": "https://github.com/tobalo/gloria-sdk#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "bun --hot examples/full-demo.ts",
    "example:news": "bun run examples/fetch-news.ts",
    "example:recaps": "bun run examples/fetch-recaps.ts",
    "example:websocket": "bun run examples/websocket.ts",
    "example:full": "bun run examples/full-demo.ts",
    "example:minimal": "bun run examples/minimal.ts",
    "test": "bun test",
    "test:examples": "bun tests/run-examples.ts",
    "test:build": "bun tests/verify-build.ts",
    "test:all": "bun test && bun run build && bun run test:build && bun run test:examples",
    "typecheck": "tsc --noEmit",
    "preship": "bun tests/pre-ship.ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "keywords": [
    "gloria",
    "ai",
    "news",
    "crypto",
    "websocket",
    "financial",
    "api",
    "sdk",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "arktype": "^2.1.22"
  }
}
